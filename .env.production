# URL бекенду (вже з /api)
VITE_API_URL=http://localhost:8080/api
# Загальні налаштування фронта
VITE_APP_NAME="Avesint OSINT Dashboard"
VITE_ENV=production
VITE_LOG_LEVEL=info

# Функціональні флаги
VITE_FEATURE_AI_ASSISTANT=false
VITE_FEATURE_DEMO_DATA=false

# Карта (приклад для OSM — якщо інше плиткове API, заміниш)
